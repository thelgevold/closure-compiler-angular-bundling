{
  "name": "closure-compiler-angular-bundling",
  "version": "1.0.0",
  "description": "Demonstrate Angular app bundled and optimized by Google Closure Compiler",
  "devDependencies": {
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/compiler-cli": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "@angular/http": "^4.0.1",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-server": "^4.0.1",
    "@angular/router": "^4.0.1",
    "@angular/tsc-wrapped": "^4.0.1",
    "google-closure-compiler": "20170218.0.0",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rxjs": "5.3.0",
    "source-map-explorer": "^1.3.3",
    "tsickle": "^0.2.3",
    "typescript": "~2.1",
    "zone.js": "angular/zone.js"
  },
  "scripts": {
    "build-webpack": "ngc && webpack --config webpack.config.js --progress",
    "build-closure": "ngc && npm run closure",
    "build-rollup": "ngc && rollup -c rollup-config.js",
    "closure": "java -jar closure-compiler/closure-compiler-1.0-SNAPSHOT.jar --flagfile closure.conf $(find node_modules/rxjs -name \\*.js) $(find built -name \\*.js)",
    "explore": "source-map-explorer dist/bundle.js dist/bundle.js.map"
  },
  "author": "Alex Eagle <alexeagle@google.com> (https://angular.io/)",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^3.3.4",
    "jquery": "^2.1.4",
    "jquery-ui": "^1.12.1",
    "webpack": "^2.3.3"
  }
}
